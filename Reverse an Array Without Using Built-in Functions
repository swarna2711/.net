using System;
class Program {
    static void Main() {
        int[] arr = { 1, 2, 3, 4, 5 };
        for (int i = 0, j = arr.Length - 1; i < j; i++, j--) {
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
        Console.WriteLine("Reversed: " + string.Join(" ", arr));
    }
}
