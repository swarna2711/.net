using System;
class Program {
    static void Main() {
        int[] arr = { 1, 2, 3, 4, 5, 6 };
        int evenSum = 0, oddSum = 0;
        foreach (int num in arr) {
            if (num % 2 == 0) evenSum += num;
            else oddSum += num;
        }
        Console.WriteLine($"Even Sum: {evenSum}, Odd Sum: {oddSum}");
    }
}
